
#!/bin/bash

# reset
reset="\e[0m"       # Text Reset

# Regular Colors
black="\e[30m"        # Black
red='\e[31m'          # Red
green="\e[32m"        # Green
yellow='\e[33m'       # Yellow
blue='\e[34m'         # Blue
purple='\e[35m'       # Purple
cyan='\e[36m'         # Cyan
white='\e[37m'        # White

printf "${blue}running command '$COMMAND'${reset}\n";
cd repo && $COMMAND

